	<script type="text/javascript">

		function iphoneTest() {

		    if (navigator.userAgent.toLowerCase().search("iphone") > -1 || navigator.userAgent.toLowerCase().search("ipod") > -1) {

		        document.getElementById('mobileStyles').innerHTML = '<link rel="stylesheet" href="www.ruud.com/styles/com-ruud-mobileStyles.css" type="text/css" />';

		    }

		}

		

		function resizeSidebar() {

            var col1 = document.getElementById('col1').offsetHeight;

            var col1Outer = document.getElementById('divCol1_Outer_Outer').offsetHeight;

            var col2 = document.getElementById('col2').offsetHeight;

            

            if (col1 < col2)

                document.getElementById('col1-inner').style.height = (col2 - col1Outer) + "px";

	    }

	</script>
